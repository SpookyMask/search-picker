{
  "name": "search picker",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "service-worker.js"
  },
  "commands": {
    "inject-script": {
      "suggested_key": "Ctrl+Q",
      "description": "Inject a script on the page"
    }
  },
  "action": {
    "default_popup": "config.html"
  },
  "permissions": ["offscreen", "geolocation"]
}